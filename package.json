{
  "name": "symlink",
  "description": "npm link together local modules && npm install remaining dependencies",
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "clux/symlink"
  },
  "keywords": [
    "link",
    "install",
    "npm",
    "dependency",
    "management"
  ],
  "scripts": {
    "test": "$(npm bin)/nodeunit --reporter=verbose test/*.js",
    "instrument": "$(npm bin)/jscoverage lib",
    "coverage": "SYMLINK_COV=1 $(npm bin)/nodeunit --reporter=lcov test/*.js",
    "coveralls": "npm run coverage | coveralls"
  },
  "bin": "bin.js",
  "main": "index.js",
  "dependencies": {
    "async": "^0.9.0",
    "interlude": "~1.1.0",
    "optimist": "~0.6.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "jscoverage": "^0.5.5",
    "nodeunit": "^0.9.0"
  },
  "bugs": {
    "url": "http://github.com/clux/symlink/issues"
  },
  "license": "MIT"
}
